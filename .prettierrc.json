{
  "printWidth": 120,
  "tabWidth": 2,
  "endOfLine": "lf",
  "useTabs": false,
  "semi": true,
  "embeddedLanguageFormatting": "off",
  "singleAttributePerLine": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "htmlWhitespaceSensitivity": "css",
  "proseWrap": "preserve",
  "insertPragma": false,
  "requirePragma": false,
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}